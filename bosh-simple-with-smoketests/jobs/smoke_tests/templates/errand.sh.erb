#!/usr/bin/env bash

pushd ..
    python=$(pwd)/packages/python/bin/python
    test_path=$(pwd)/packages/acceptance_tests_pkg/acceptance-tests/
    export CF_PATH=$(pwd)/packages/cli/bin/cf
popd

export API_URL=<%= p("cf.api_url") %>
export ADMIN_USER=<%= p("cf.admin_username") %>
export ADMIN_PASS=<%= p("cf.admin_password") %>
export SKIP_SSL=<%= p("cf.skip_ssl_validation", false) %>

${python} ${test_path}/test.py
